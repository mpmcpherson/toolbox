using System;

public enum Color
{
    Red,
    Blue,
    Green
}

class Solution
{

    static Color[,] colors = new Color[5, 5]
    {
        { Color.Green, Color.Blue, Color.Red, Color.Green, Color.Green },
        { Color.Green, Color.Blue, Color.Red, Color.Green, Color.Blue },
        { Color.Green, Color.Green, Color.Blue, Color.Red, Color.Blue },
        { Color.Red, Color.Red, Color.Red, Color.Red, Color.Blue },
        { Color.Green, Color.Green, Color.Green, Color.Blue, Color.Blue }
    };

    // Which color occupies the largest contiguous, non-diagonal space?
    
    static void Main(string[] args)
    {
        
        var count = 0;
        var currCount = 0;
        String color = "null";
        String currColor = "null";
        
        
        for (var i = 0; i < colors.GetLength(0); i++)
        {
            for (var j = 0; j < colors.GetLength(1); j++)
            {
                if(colors[i,j]==Color.Green&&currColor.Equals("green"))
                {
                    currCount++;
                }
                if( colors[i,j]==Color.Red&&currColor.Equals("red")){
                    currCount++;
                }
                if(currColor.Equals("blue")){
                    currCount++;
                }
                
                if(currCount > count){
                    color = currColor;
                    count = currCount;
                }
                
                Console.WriteLine(colors[i,j]);
            }
        }
        Console.WriteLine();
        Console.WriteLine(color);
    }
}